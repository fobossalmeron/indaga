{
  "format": "custom",
  "id": "happening",
  "label": "Happening",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "title": {
        "type": "Text",
        "config": {
          "placeholder": "",
          "label": "Título",
          "useAsTitle": true
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Imagen",
          "constraint": {},
          "thumbnails": []
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Categoria",
          "placeholder": "",
          "options": ["Arte", "Cultura", "Música", "Gastronomía", "Cine"]
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Fecha del evento",
          "placeholder": ""
        }
      },
      "end_date": {
        "type": "Date",
        "config": {
          "label": "Fecha de finalización (opcional)",
          "placeholder": "Si el evento tiene múltiples días"
        }
      },
      "time": {
        "type": "StructuredText",
        "config": {
          "label": "Horario (opcional)",
          "placeholder": "12:00hrs",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,list-item,o-list-item"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Descripción",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,hyperlink,list-item,o-list-item"
        }
      },
      "location_name": {
        "type": "Text",
        "config": {
          "label": "Nombre de Locación ",
          "placeholder": ""
        }
      },
      "location_url": {
        "type": "Link",
        "config": {
          "label": "URL de Locación",
          "placeholder": "https://... ",
          "allowTargetBlank": true,
          "select": "web"
        }
      },
      "cost": {
        "type": "StructuredText",
        "config": {
          "label": "Costo (opcional)",
          "placeholder": "$130 pesos",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,list-item,o-list-item"
        }
      }
    }
  }
}
