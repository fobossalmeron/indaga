{
  "format": "custom",
  "id": "lugar",
  "label": "Lugar",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "nombre": {
        "type": "Text",
        "config": {
          "label": "Nombre",
          "placeholder": "",
          "useAsTitle": true
        }
      },
      "area": {
        "type": "Text",
        "config": {
          "label": "Área de la ciudad",
          "placeholder": "Centro de monterrey"
        }
      },
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "categoria": {
        "type": "Select",
        "config": {
          "label": "Categoria",
          "placeholder": "Categoría",
          "options": [
            "Bares & Cantinas",
            "Monumentos Históricos",
            "Cafeterías",
            "Música en Vivo",
            "Espacios de Arte",
            "Restaurantes"
          ]
        }
      },
      "link": {
        "type": "Link",
        "config": {
          "label": "Link",
          "placeholder": "https://... ",
          "allowTargetBlank": true,
          "select": "web"
        }
      },
      "mapLink": {
        "type": "Link",
        "config": {
          "label": "Link de mapa",
          "placeholder": "Google maps url",
          "allowTargetBlank": true,
          "select": null
        }
      },
      "capsuleLink": {
        "type": "Link",
        "config": {
          "label": "Link de cápsula",
          "placeholder": "",
          "allowTargetBlank": true,
          "select": null
        }
      }
    }
  }
}
